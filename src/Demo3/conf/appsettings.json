{
  "webApiConfiguration": {
    "portNumber": 5110
  },
  "swagger": {
    "useSwagger": true
  },
  "metrics": {
    "enabled": true,
    "reporter": "prometheus"
  },
  "db": [
    {
      "nick": "default",
      "dbServerType": "SQLite",
      "ConnectionString": "Data Source=database.db",
      "useCallContext": false
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "logger": {
    "listeners": [
      {
        "type": "console",
        "category": "All",
        "minLevel": "Debug"
      }
    ]
  }
}
